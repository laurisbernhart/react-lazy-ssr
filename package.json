{
  "name": "react-lazy-ssr",
  "version": "0.2.4",
  "description": "React.lazy substitute which works with server-side rendering",
  "main": "index.js",
  "browser": "es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./index.js"
    },
    "./babel": {
      "import": "./es/babel.js",
      "require": "./babel.js"
    },
    "./server": {
      "import": "./es/server.js",
      "require": "./server.js"
    },
    "./webpack": {
      "import": "./es/webpack.js",
      "require": "./webpack.js"
    },
    "./es": "./es/index.js",
    "./es/babel": "./es/babel.js",
    "./es/server": "./es/server.js",
    "./es/webpack": "./es/webpack.js"
  },
  "sideEffects": false,
  "author": {
    "name": "Overlook Motel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/overlookmotel/react-lazy-ssr.git"
  },
  "bugs": {
    "url": "https://github.com/overlookmotel/react-lazy-ssr/issues"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "is-it-type": "^3.1.1",
    "lodash": "^4.17.15",
    "react-async-ssr": "^0.7.1",
    "tiny-invariant": "^1.1.0"
  },
  "peerDependencies": {
    "react": "16.8.0 - 16.9.x"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-for-of": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@overlookmotel/eslint-config": "^7.1.2",
    "@overlookmotel/eslint-config-jest": "^4.0.1",
    "@overlookmotel/eslint-config-node": "^2.0.0",
    "@overlookmotel/eslint-config-react": "^7.0.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-test": "^0.2.3",
    "cheerio": "^1.0.0-rc.3",
    "coveralls": "^3.0.11",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "fs-extra": "^9.0.0",
    "jest": "^25.2.7",
    "jest-extended": "^0.11.5",
    "jsdom": "^16.2.2",
    "parse-node-version": "^1.0.1",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "rollup": "^2.3.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^5.3.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "keywords": [
    "react",
    "lazy",
    "load",
    "server",
    "render"
  ],
  "scripts": {
    "build": "rm -rf ./dist && rollup -c",
    "prepublishOnly": "npm run build",
    "test": "npm run lint && npm run test-src-dev",
    "test-all": "npm run test-all-dev && npm run test-all-prod",
    "test-all-dev": "npm run test-src-dev && npm run test-cjs-dev && npm run test-esm-dev && npm run test-umd-dev",
    "test-all-prod": "npm run test-src-prod && npm run test-cjs-prod && npm run test-esm-prod && npm run test-umd-prod",
    "test-src": "npm run test-src-dev && npm run test-src-prod",
    "test-src-dev": "npm run test-main",
    "test-src-prod": "npm run test-prod",
    "test-cjs": "npm run test-cjs-dev && npm run test-cjs-prod",
    "test-cjs-dev": "cross-env TEST_ENV=cjs npm run test-main",
    "test-cjs-prod": "cross-env TEST_ENV=cjs npm run test-prod",
    "test-esm": "npm run test-esm-dev && npm run test-esm-prod",
    "test-esm-dev": "cross-env TEST_ENV=esm npm run test-main",
    "test-esm-prod": "cross-env TEST_ENV=esm npm run test-prod",
    "test-umd": "npm run test-umd-dev && npm run test-umd-prod",
    "test-umd-dev": "cross-env TEST_ENV=umd npm run test-main",
    "test-umd-prod": "cross-env TEST_ENV=umd npm run test-prod",
    "test-main": "npm run build-fixtures && npm run test-only",
    "test-prod": "cross-env NODE_ENV=production npm run test-main",
    "test-only": "cross-env BABEL_ENV=test jest",
    "build-fixtures": "cd test/fixtures && rm -rf build && node ../support/build.js && cross-env BABEL_ENV=test webpack; cd ../..",
    "lint": "eslint '*.{js,jsx}' '.*.{js,jsx}' '**/*.{js,jsx}' '**/.*.{js,jsx}'",
    "lint-fix": "eslint '*.{js,jsx}' '.*.{js,jsx}' '**/*.{js,jsx}' '**/.*.{js,jsx}' --fix",
    "cover": "npm run cover-main && rm -rf coverage",
    "coveralls": "npm run build && npm run cover-main && cat coverage/lcov.info | coveralls && rm -rf coverage",
    "cover-main": "npm run build-fixtures && npm run cover-only",
    "cover-only": "cross-env COVERAGE=true BABEL_ENV=test jest --coverage",
    "ci": "if [ $COVERAGE ]; then npm run coveralls; else npm run ci-main; fi",
    "ci-main": "npm run lint && npm run build && npm run test-all"
  },
  "engines": {
    "node": ">=10"
  },
  "readmeFilename": "README.md",
  "license": "MIT"
}
